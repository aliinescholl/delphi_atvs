unit UCarRental;

interface

uses
  UInvoice, UVehicle;

type
  TcarRental = class
    private
    FStart: TDateTime;
    FFinish:TDateTime;

    FVehicle:TVehicle;
    FInVOice:TInVoice;

    public
    property Start: TDateTime read GetStart write SetStart;
    property Finish: TDateTime read GetFinish write SetFinish;
    property Vehicle: TVehicle read GetVehicle write SetVehicle;
    property InVoice: TInVoice read FInVoice write FInVoice;
  end;

implementation

end.
