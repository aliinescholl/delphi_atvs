  {$R *.dfm}
  procedure TForm6.ButtonClick(Sender:TOb)
var
  xCachorro : Tcachorro;
begin
  xCachorro:=Tcachorro.Create;
  try
    xCachorro.MeuSomEh;
  finally
     FreeAndNil(xCachorro);

