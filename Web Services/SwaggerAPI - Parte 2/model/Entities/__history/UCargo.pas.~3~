unit UCargo;

interface

type
 TCargo = class
  private
    FID: Integer;
    FDescricao: String;
  public
  [SwagProp('Cargo Id', True)]
    property Id:Integer read FID write FID;

  [SwagProp('Descrição cargo', True)]
    property Nome: String read FDescricao write FDescricao;
end;
  TAPIError = class
    Private
      Ferror: String;
    public
      property Error: String read FError write FError;
  end;

implementation

end.
